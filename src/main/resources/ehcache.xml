<ehcache>

    <!-- Default ehcache -->
    <defaultCache
        maxElementsInMemory="100"
        eternal="true"
        overflowToDisk="false"
        memoryStoreEvictionPolicy="LFU"/>

    <cache name="calendarCache"
        maxElementsInMemory="100"
        eternal="false"
        timeToLiveSeconds="1"
        overflowToDisk="false"
        memoryStoreEvictionPolicy="LRU"/>

    
</ehcache>